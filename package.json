{
  "name": "myapp-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "pre-commit": "lint-staged",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,css,scss,less,md,json}'"
  },
